{
  "name": "Clout Block",
  "version": "0.0.1",
  "description": "Block out the clout by removing stats from social media",
  "manifest_version": 2,
  "permissions": ["tabs", "https://www.facebook.com/*"],
  "content_scripts": [
    {
      "matches": ["https://www.facebook.com/*"],
      "css": ["style.css"]
    }
  ]
}
